<?xml version="1.0" encoding="UTF-8"?>
<conf xmlns:xs="http://www.w3.org/2001/XMLSchema"
      xmlns:xd="http://www.oxygenxml.com/ns/doc/xsl"
      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
      xmlns:xslq="https://github.com/mricaud/xsl-quality"
      xmlns="https://github.com/mricaud/xsl-quality">
   <param name="xslqual-FunctionComplexity-maxSize">50</param>
   <pattern idref="xslqual_mukulgandhi" active="true">
      <rule idref="xslqual_stylesheet" active="true">
         <assert idref="xslqual-RedundantNamespaceDeclarations" active="true"/>
         <report idref="xslqual-TooManySmallTemplates" active="true"/>
         <report idref="xslqual-MonolithicDesign" active="true"/>
         <report idref="xslqual-NotUsingSchemaTypes" active="true"/>
      </rule>
      <rule idref="xslqual_output" active="true">
         <report idref="xslqual-OutputMethodXml" active="true"/>
      </rule>
      <rule idref="xslqual_variable" active="true">
         <report idref="xslqual-SettingValueOfVariableIncorrectly" active="true"/>
         <assert idref="xslqual-UnusedVariable" active="true"/>
      </rule>
      <rule idref="xslqual_param" active="true">
         <report idref="xslqual-SettingValueOfParamIncorrectly" active="true"/>
         <assert idref="xslqual-UnusedParameter" active="true"/>
      </rule>
      <rule idref="xslqual_instructions" active="true">
         <report idref="xslqual-EmptyContentInInstructions" active="true"/>
      </rule>
      <rule idref="xslqual-functions" active="true">
         <assert idref="xslqual-UnusedFunction" active="true"/>
         <report idref="xslqual-FunctionComplexity" active="true"/>
      </rule>
      <rule idref="xslqual-template" active="true">
         <report idref="xslqual-UnusedNamedTemplate" active="true"/>
         <report idref="xslqual-TemplateComplexity" active="true"/>
      </rule>
      <rule idref="xslqual-element" active="true">
         <report idref="xslqual-NotCreatingElementCorrectly" active="true"/>
      </rule>
      <rule idref="xslqual-apply-templates" active="true">
         <report idref="xslqual-ConfusingVariableAndNode" active="true"/>
      </rule>
      <rule idref="xslqual-attributes" active="true">
         <report idref="xslqual-DontUseDoubleSlashOperatorNearRoot" active="true"/>
         <report idref="xslqual-DontUseDoubleSlashOperator" active="true"/>
         <report idref="xslqual-UsingNameOrLocalNameFunction" active="true"/>
         <report idref="xslqual-IncorrectUseOfBooleanConstants" active="true"/>
         <report idref="xslqual-UsingNamespaceAxis" active="true"/>
         <report idref="xslqual-CanUseAbbreviatedAxisSpecifier" active="true"/>
         <report idref="xslqual-UsingDisableOutputEscaping" active="true"/>
      </rule>
   </pattern>
   <pattern idref="xslt-quality_common" active="true">
      <report idref="xslt-quality_avoid-for-each" active="true"/>
      <report idref="xslt-quality_use-resolve-uri-instead-of-concat" active="true"/>
   </pattern>
   <pattern idref="xslt-quality_documentation" active="true">
      <rule idref="xslt-quality_documentation-stylesheet" active="true">
         <assert idref="xslt-quality_documentation-stylesheet-docblock" active="true"/>
      </rule>
   </pattern>
   <pattern idref="xslt-quality_namespaces" active="true">
      <rule idref="xslt-quality_namespaces_name" active="true">
         <assert idref="xslt-quality_ns-global-statements-need-prefix" active="true"/>
      </rule>
      <rule idref="xslt-quality_namespaces_mode" active="true">
         <assert idref="xslt-quality_ns-mode-statements-need-prefix" active="true"/>
      </rule>
      <rule idref="xslt-quality_namespaces_xpath" active="true">
         <report idref="xslt-quality_ns-do-not-use-wildcard-prefix" active="true"/>
      </rule>
   </pattern>
   <pattern idref="xslt-quality_typing" active="true">
      <report idref="xslt-quality_typing-with-as-attribute" active="true"/>
   </pattern>
   <pattern idref="xslt-quality_writing" active="true">
      <report idref="xslt-quality_writing-use-select-attribute-when-possible"
              active="true"/>
   </pattern>
   <pattern idref="xslt-quality_xslt-3.0" active="true">
      <report idref="xslt-quality_xslt-3.0-import-first" active="true"/>
   </pattern>
   <pattern idref="xslt-quality_debug" active="false">
      <rule idref="xslt-quality_debug-root" active="true"/>
   </pattern>
</conf>
